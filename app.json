{
  "expo": {
    "name": "Trybe",
    "slug": "trybe-rn",
    "version": "1.0.0",
    "scheme": "trybe-rn",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/ReklameScript-Regular.otf",
            "./assets/fonts/SourceSansPro-Regular.otf",
            "./assets/fonts/Taprom-Regular.ttf"
          ]
        }
      ],
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon-orig.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.djscruggs.trybern",
      "infoPlist": {
        "CFBundleDisplayName": "Trybe",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can select a profile picture.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save photos to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-orig.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.djscruggs.trybern"
    },
    "extra": {
      "eas": {
        "projectId": "5ab78094-cdc6-4004-b56c-ad603729ba6a"
      }
    }
  }
}
